{

  "volumes": null,
  "id": "/micro-ci-test",
  "cmd": null,
  "args": null,
  "user": null,
  "env": {
    "APPROLE": {
      "secret": "role"
    }
  },
  "instances": 1,
  "cpus": 1,
  "mem": 1024,
  "disk": 0,
  "gpus": 0,
  "executor": null,
  "constraints": null,
  "fetch": [
    {
	"uri": "file:///opt/docker/docker.tar.gz"      
    }
  ],
  "storeUrls": null,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "docker": {
      "image": "stratioci/micro-ci-test:1.0.0-SNAPSHOT",
      "forcePullImage": true,
      "privileged": false,
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "readinessChecks": null,
  "dependencies": null,
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_PORT": "10188"
  },
  "acceptedResourceRoles": null,
  "residency": null,
  "secrets": {
    "role": {
      "source": "open"
    }
  },
  "taskKillGracePeriodSeconds": null,
  "requirePorts": false
}
